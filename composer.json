{
    "name": "huexotzinca/csvtranslator",
    "type": "library",
    "description": "A simple Reader and Parser for populate DB from CSV file.",
    "keywords": [
        "csv",
        "reader",
        "parser"
    ],
    "homepage": "http://huexotzinca.github.io/CSVTranslator",
    "support": {
        "issues": "https://github.com/huexotzinca/CSVTranslator/issues",
        "source": "https://github.com/huexotzinca/CSVTranslator"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Neftali Bautista",
            "email": "dev@neftalibautista.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/filesystem": "~3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2",
        "phpunit/phpunit": "~4.0 || ~5.0"
    },
    "autoload": {
        "psr-4": {
            "CSVTranslator\\": "src/CSVTranslator/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "php vendor/bin/phpunit; php vendor/bin/php-cs-fixer fix -v --diff --dry-run;",
        "phpunit": "php vendor/bin/phpunit;",
        "phpcs": "php vendor/bin/php-cs-fixer fix -v --diff --dry-run;"
    }
}
